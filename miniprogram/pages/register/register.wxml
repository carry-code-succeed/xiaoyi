<!--pages/zhuce/zhuce.wxml-->
<view class="shanghuaxian"></view>
<view>
  <view class="box1">
    <text decode="true">账号:</text>
    <input placeholder="注册账号 4-12位字母、数字组合" type="text" bindinput="zhanghao" bindblur="xhyz" bindblur="xhyz"></input>
  </view>
  <view class="box1">
    <text>密码:</text>
    <input placeholder="密码 6-20位字母、数字、下划线组合" type="text" bindinput="mima1" bindblur="xhyz"> bindblur="xhyz" </input>
  </view>
  <view class="box1">
    <text>确认密码:</text>
    <input placeholder="确认密码"  type="text" bindinput="mima2" bindblur="xhyz" bindblur="xhyz"></input>
  </view>
  <view class="box1">
    <text>昵称:</text>
    <input placeholder="请输入用户昵称" type="text" bindinput="name" bindblur="xhyz" bindfocus="xhyz"></input>
  </view>
   <view class="box1">
   <text>学号:</text>
  <input placeholder="请输入学号" type="text" bindinput="xuehao" bindblur="xhyz" bindblur="xhyz"></input>
  </view>
  <view class="box1">
    <text>姓名:</text>
    <input placeholder="请输入真实姓名" type="text" bindinput="xingming" bindblur="xhyz" bindfocus="xhyz"></input>
  </view>
</view>
<view class="box2">
  <text>{{meg}}</text>
</view>

<view class="box3" >
  <button disabled="{{jump}}" bindtap="zhuce">注册</button>
</view>
<view class="box2">
<navigator>《校易注册协议》</navigator>
</view>
<view class="box2">
  
  <checkbox-group bindchange="checkboxChange">
    <checkbox value="tongyi">
    同意注册协议
    </checkbox>
  </checkbox-group>
  
</view>


