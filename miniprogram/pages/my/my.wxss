page{
  background-color: white;
}
.user_info_warp{
  height: 45vh;
  background-color: #027DD5;
  overflow: hidden;
}
.user_img_wrap{
  position: relative;
  
}
.user_bg{
  width: 100%;
  height: 50vh;
  filter: blur(10rpx);
}
.user_info{
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  top: 15%;
  text-align: center;
}
.user_icon{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.user_name{
  color: white;
  margin-top: 40rpx;
  font-size: 40rpx;
}
.user_content{
  position: relative;
}
.user_main{
  position: absolute;
  width: 90%;
  left: 50%;
  transform: translate(-50%);
  top: 30rpx;
}
.history_warp{
  display: flex;
  margin-bottom: 40rpx;
}
.history_warp navigator{
  flex: 1;
  text-align: center;
}
.history_img{

  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  text-align: center;

  transform: translate(25%);
  
}
.history_img view{
  font-size: 80rpx;
  width: 100rpx;
  height: 100rpx;

}
.history_name{
  font-size: 30rpx;
  width: 150rpx;
}
